<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 这里namespace必须是接口的路径，不然要运行的时候要报错 “is not known to the MapperRegistry” -->
<mapper namespace="com.mazing.services.order.base.dao.OverAcceptNotifiedOrderDao">

	<!-- 新增 -->
	<insert id="save" parameterType="com.mazing.services.order.base.entry.OverAcceptNotifiedOrderPO" useGeneratedKeys="false" >
		insert into mz_order.mz_over_accept_notified_order (order_no,notify_times,create_time) values (#{orderNo},#{notifyTimes},#{createTime})
	</insert>

	<!-- 根据主键，删除数据
	<delete id="deleteById" parameterType="Map">
		DELETE FROM mz_order.mz_over_accept_notified_order WHERE order_no=#{param1}
	</delete>
	 -->

	<!-- 更新 -->
	<update id="update" parameterType="com.mazing.services.order.base.entry.OverAcceptNotifiedOrderPO">
		update mz_order.mz_over_accept_notified_order set notify_times=#{notifyTimes} where order_no=#{orderNo}
	</update>

	<!-- 根据主键读取记录（主键可能有多个） -->
	<select id="getById" parameterType="Map" resultType="com.mazing.services.order.base.entry.OverAcceptNotifiedOrderPO">
		SELECT * FROM mz_order.mz_over_accept_notified_order WHERE order_no=#{param1}
	</select>

	<!-- 查询全部记录 -->
	<select id="listAll" resultType="com.mazing.services.order.base.entry.OverAcceptNotifiedOrderPO">
		SELECT * FROM mz_order.mz_over_accept_notified_order
	</select>

</mapper>
